<?xml version="1.0" encoding="utf-8"?>
<com.greenorange.gooutdoor.View.layout.TimerRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
      android:orientation="vertical"
      android:layout_width="match_parent"
      android:layout_height="match_parent">
      <com.greenorange.gooutdoor.framework.widget.RevealView.CircularRevealView
          android:id="@+id/circular"
          android:layout_width="fill_parent"
          android:layout_height="fill_parent">
      </com.greenorange.gooutdoor.framework.widget.RevealView.CircularRevealView>
      <RelativeLayout
          android:id="@+id/touch"
          android:gravity="center"
          android:layout_width="fill_parent"
          android:layout_height="fill_parent">
          <TextView
              android:id="@+id/txtNumber"
              android:layout_marginTop="20dip"
              android:layout_width="fill_parent"
              android:layout_height="fill_parent"
              android:gravity="center"
              android:textSize="200sp"
              android:textColor="#FFFFFF"
              style="@style/text_shadow_style"/>
      </RelativeLayout>
</com.greenorange.gooutdoor.View.layout.TimerRelativeLayout>